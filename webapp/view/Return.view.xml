									<Button text="{i18n>Btn_Submit}" press="onPress" enabled="{localModel>/readmode}">
										<layoutData>
											<FlexItemData growFactor="1"/>
										</layoutData>
									</Button>
									<Button text="{i18n>Btn_Digit}" press="onSign" visible="{localModel>/displaymode}">
										<layoutData>
											<FlexItemData growFactor="1"/>
										</layoutData>
									</Button>
								</HBox>
							</f:content>
						</f:SimpleForm>
					</VBox>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>